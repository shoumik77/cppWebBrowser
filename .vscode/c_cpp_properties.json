{
    "configurations": [
      {
        "name": "macOS",
        "includePath": [
          "${workspaceFolder}/**",
          "/opt/homebrew/include",
          "/opt/homebrew/include/QtCore",
          "/opt/homebrew/include/QtWidgets",
          "/opt/homebrew/include/QtWebEngineWidgets",
          "/opt/homebrew/include/QtGui"
        ],
        "defines": [
          "QT_CORE_LIB",
          "QT_GUI_LIB",
          "QT_WIDGETS_LIB",
          "QT_WEBENGINEWIDGETS_LIB"
